aqseq 321


# 1 "mc_line 73 file /root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/mlevhsqcetf3gp3d exp. def. part of mc cmd. before ze"
; dimension 3D; AQ_mode  (F2) Echo-Antiecho (F1) States-TPPI
define delay MCWRK
define delay MCREST
define delay d0orig
"d0orig=d0"
define delay d10orig
"d10orig=d10"
define loopcounter t2loop
"t2loop=0"
define loopcounter t1loop
"t1loop=0"
define loopcounter ph1loop
"ph1loop=0"
define loopcounter ph2loop
"ph2loop=0"
define loopcounter ST1CNT
"ST1CNT = trunc(td1 / 2)"
define loopcounter ST2CNT
"ST2CNT = trunc(td2 / 2)"
"MCREST = d1 - d1"
"MCWRK = 0.029412*d1"
# 73 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/mlevhsqcetf3gp3d"
1 ze
# 1 "mc_line 73 file /root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/mlevhsqcetf3gp3d exp. def. of mc cmd. after ze"
LBLAV, MCWRK
      "d0=d0orig + t1loop * in0 "
      "phval7 = (ph1loop % 4) * 90"
      MCWRK ip7 + phval7
      "phval8 = (ph1loop % 4) * 90"
      MCWRK ip8 + phval8
      "d10=d10orig + t2loop * in10 "
      "phval3 = (t2loop * 180)%360"
      MCWRK ip3 + phval3
      "phval6 = (t2loop * 180)%360"
      MCWRK ip6 + phval6
      "phval31 = (t2loop * 180)%360"
      MCWRK ip31 + phval31
      MCWRK
      "phval0 = ph2loop * 1"
      MCWRK setgrad EA
# 74 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/mlevhsqcetf3gp3d"
  d11 pl16:f3
# 1 "mc_line 75 file /root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/mlevhsqcetf3gp3d exp. start label for mc cmd."
2 MCWRK  * 8 do:f3
LBLF2, MCWRK  * 8
LBLST2, MCWRK  * 9
LBLF1, MCWRK  * 8
LBLST1, MCWRK 
  MCREST
# 76 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/mlevhsqcetf3gp3d"
3 d12 pl3:f3
  (p1 ph7)
  DELTA
  (p2 ph8)
  d0
# 83 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/mlevhsqcetf3gp3d"
  (center (p14:sp3 ph1):f2 (p22 ph1):f3 )


# 88 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/mlevhsqcetf3gp3d"
  d0
  d12 pl10:f1
  (p17 ph26)
						;begin MLEV17
6 (p6 ph22 p7 ph23 p6 ph22)
  (p6 ph24 p7 ph25 p6 ph24)
  (p6 ph24 p7 ph25 p6 ph24)
  (p6 ph22 p7 ph23 p6 ph22)
  (p6 ph24 p7 ph25 p6 ph24)
  (p6 ph24 p7 ph25 p6 ph24)
  (p6 ph22 p7 ph23 p6 ph22)
  (p6 ph22 p7 ph23 p6 ph22)
  (p6 ph24 p7 ph25 p6 ph24)
  (p6 ph22 p7 ph23 p6 ph22)
  (p6 ph22 p7 ph23 p6 ph22)
  (p6 ph24 p7 ph25 p6 ph24)
  (p6 ph22 p7 ph23 p6 ph22)
  (p6 ph22 p7 ph23 p6 ph22)
  (p6 ph24 p7 ph25 p6 ph24)
  (p6 ph24 p7 ph25 p6 ph24)
  (p5 ph23)
  lo to 6 times l1 
						;end MLEV17
  (p17 ph26) 
  d26 pl1:f1
  (center (p2 ph1) (p22 ph6):f3 )
  d26 setrtp0|0 ctrlgrad 0
  p28 ph1
  d13
  (p1 ph2) (p21 ph3):f3
  d10 


  (center (p2 ph5) (p14:sp3 ph1):f2 )
# 126 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/mlevhsqcetf3gp3d"
  d10
  p16:gp1*EA
  d16
  (p22 ph4):f3
  DELTA1
  (ralign (p1 ph1) (p21 ph4):f3 )
  d26
  (center (p2 ph1) (p22 ph1):f3 )
  d13
  p16:gp2
  DELTA2 pl16:f3
  4u setrtp0^0 ctrlgrad 4047
  go=2 ph31 cpd3:f3
# 1 "mc_line 139 file /root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/mlevhsqcetf3gp3d exp. mc cmd. in line"
  MCWRK  do:f3 wr #0 if #0 zd 

  "ph2loop+=1"
      "d0=d0orig + t1loop * in0 "
      "phval7 = (ph1loop % 4) * 90"
      MCWRK ip7 + phval7
      "phval8 = (ph1loop % 4) * 90"
      MCWRK ip8 + phval8
      "d10=d10orig + t2loop * in10 "
      "phval3 = (t2loop * 180)%360"
      MCWRK ip3 + phval3
      "phval6 = (t2loop * 180)%360"
      MCWRK ip6 + phval6
      "phval31 = (t2loop * 180)%360"
      MCWRK ip31 + phval31
      MCWRK
      "phval0 = ph2loop * 1"
      MCWRK setgrad EA
  lo to LBLF2 times 2
  MCWRK 

  "t2loop+=1"
      "d0=d0orig + t1loop * in0 "
      "phval7 = (ph1loop % 4) * 90"
      MCWRK ip7 + phval7
      "phval8 = (ph1loop % 4) * 90"
      MCWRK ip8 + phval8
      "d10=d10orig + t2loop * in10 "
      "phval3 = (t2loop * 180)%360"
      MCWRK ip3 + phval3
      "phval6 = (t2loop * 180)%360"
      MCWRK ip6 + phval6
      "phval31 = (t2loop * 180)%360"
      MCWRK ip31 + phval31
      MCWRK
      "phval0 = ph2loop * 1"
      MCWRK setgrad EA
  lo to LBLST2 times ST2CNT
  MCWRK 
  "t2loop=0"
  "ph2loop=0"
  MCWRK 

  "ph1loop+=1"
      "d0=d0orig + t1loop * in0 "
      "phval7 = (ph1loop % 4) * 90"
      MCWRK ip7 + phval7
      "phval8 = (ph1loop % 4) * 90"
      MCWRK ip8 + phval8
      "d10=d10orig + t2loop * in10 "
      "phval3 = (t2loop * 180)%360"
      MCWRK ip3 + phval3
      "phval6 = (t2loop * 180)%360"
      MCWRK ip6 + phval6
      "phval31 = (t2loop * 180)%360"
      MCWRK ip31 + phval31
      MCWRK
      "phval0 = ph2loop * 1"
      MCWRK setgrad EA
  lo to LBLF1 times 2
  MCWRK 

  "t1loop+=1"
      "d0=d0orig + t1loop * in0 "
      "phval7 = (ph1loop % 4) * 90"
      MCWRK ip7 + phval7
      "phval8 = (ph1loop % 4) * 90"
      MCWRK ip8 + phval8
      "d10=d10orig + t2loop * in10 "
      "phval3 = (t2loop * 180)%360"
      MCWRK ip3 + phval3
      "phval6 = (t2loop * 180)%360"
      MCWRK ip6 + phval6
      "phval31 = (t2loop * 180)%360"
      MCWRK ip31 + phval31
      MCWRK
      "phval0 = ph2loop * 1"
      MCWRK setgrad EA
  lo to LBLST1 times ST1CNT
  MCWRK 
  "t1loop=0"
  "ph1loop=0"
  MCWRK rf #0
  lo to LBLAV times tdav
# 142 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/mlevhsqcetf3gp3d"
exit
   

ph1=0 
ph2=1
ph3=0 2
ph4=0 0 0 0 0 0 0 0 2 2 2 2 2 2 2 2
ph5=0 0 2 2
ph6=0
ph7=0 0 0 0 2 2 2 2
ph8=1 1 1 1 3 3 3 3
ph22=0
ph23=1
ph24=2
ph25=3
ph26=1
ph31=0 2 0 2 2 0 2 0 2 0 2 0 0 2 0 2
 

;pl1 : f1 channel - power level for pulse (default)
;pl3 : f3 channel - power level for pulse (default)
;pl10: f1 channel - power level for TOCSY-spinlock
;pl16: f3 channel - power level for CPD/BB decoupling
;sp3: f2 channel - shaped pulse 180 degree (adiabatic)
;p1 : f1 channel -  90 degree high power pulse
;p2 : f1 channel - 180 degree high power pulse
;p5 : f1 channel -  60 degree low power pulse
;p6 : f1 channel -  90 degree low power pulse
;p7 : f1 channel - 180 degree low power pulse
;p14: f2 channel - 180 degree shaped pulse for inversion (adiabatic)
;p16: homospoil/gradient pulse
;p17: f1 channel - trim pulse                             [2.5 msec]
;p21: f3 channel -  90 degree high power pulse
;p22: f3 channel - 180 degree high power pulse
;p28: f1 channel - trim pulse
;d0 : incremented delay (F1 in 3D)                     [3 usec]
;d1 : relaxation delay; 1-5 * T1
;d9 : TOCSY mixing time
;d10: incremented delay (F2 in 3D)                     [3 usec]
;d11: delay for disk I/O                             [30 msec]
;d12: delay for power switching                      [20 usec]
;d13: short delay                                    [4 usec]
;d16: delay for homospoil/gradient recovery
;d26: 1/(4J)YH
;cnst4: = J(YH)
;l1: loop for MLEV cycle: (((p6*64) + p5) * l1) + (p17*2) = mixing time
;inf1: 1/SW(H) = 2 * DW(H)
;inf2: 1/SW(X) = 2 * DW(X)
;in0: 1/(2 * SW(H)) = DW(H)
;nd0: 2
;in10: 1/(2 * SW(X)) = DW(X)
;nd10: 2
;ns: 8 * n
;ds: >= 16
;td1: number of experiments in F1
;td2: number of experiments in F2
;FnMODE: States-TPPI (or TPPI) in F1
;FnMODE: echo-antiecho in F2
;cpd3: decoupling according to sequence defined by cpdprg3
;pcpd3: f3 channel - 90 degree pulse for decoupling sequence
# 204 "/root/.topspin-BladeEpu/local_acqu/simulDirs/ppDirs/mlevhsqcetf3gp3d"
;use gradient ratio:    gp 1 : gp 2
;                         80 : 20.1    for C-13
;                         80 :  8.1    for N-15

;for z-only gradients:
;gpz1: 80%
;gpz2: 20.1% for C-13, 8.1% for N-15

;use gradient files:   
;gpnam1: SMSQ10.100
;gpnam2: SMSQ10.100
