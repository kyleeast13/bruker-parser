# 80 "/root/.topspin-BladeEpu/local_acqu/ppDirs/Delay.incl"
;$Id: Delay.incl,v 1.15 2013/08/30 09:42:06 ber Exp $
# 33 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi" 2


"p22=p21*2"
"d11=30m"
"d26=1s/(cnst4*4)"
# 39 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
"p29=300u"


# 43 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
"p41=(bwfac25/(cnst55*cnst51*bf1))*1000000"
"spw25=plw1/((p41*90.0)/(p1*totrot25))*((p41*90.0)/(p1*totrot25))*(integfac25*integfac25)"
"spw27=plw1/((p41*90.0)/(p1*totrot27))*((p41*90.0)/(p1*totrot27))*(integfac27*integfac27)"
"spoal25=1"
"spoal27=0"

"p42=(bwfac26/(cnst55*cnst52*bf1))*1000000"
"spw26=plw1/((p42*90.0)/(p1*totrot26))*((p42*90.0)/(p1*totrot26))*(integfac26*integfac26)"
"spoal26=0.5"

"p43=(bwfac28/(cnst55*cnst53*bf1))*1000000"
"spw28=plw1/((p43*90.0)/(p1*totrot28))*((p43*90.0)/(p1*totrot28))*(integfac28*integfac28)"
"spw29=plw1/((p43*90.0)/(p1*totrot29))*((p43*90.0)/(p1*totrot29))*(integfac29*integfac29)"
"spoal28=1"
"spoal29=0"
# 61 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
"d0=3u"

"in0=inf1/2"

# 66 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
"DELTA1=d26-p29-d16-p41*cnst41-p42*cnst40"
"DELTA2=d26-p19-d16-p42/2"
"DELTA3=d26-p29-d16-p43*cnst43-p42*cnst42"
"DELTA4=p19+d16+de+8u"


"DELTA=p43-d0*2-p19-larger(p8,p44)"
# 78 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
"spoff25=bf1*(cnst54/1000000)-o1"
"spoff26=bf1*(cnst54/1000000)-o1"
"spoff27=bf1*(cnst54/1000000)-o1"
"spoff28=bf1*(cnst54/1000000)-o1"
"spoff29=bf1*(cnst54/1000000)-o1"
"spoff30=0"


# 1 "mc_line 86 file /root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi exp. def. part of mc cmd. before ze"
; dimension 2D; AQ_mode  (F1) Echo-Antiecho
define delay MCWRK
define delay MCREST
define delay d0orig
"d0orig=d0"
define loopcounter t1loop
"t1loop=0"
define loopcounter ph1loop
"ph1loop=0"
define loopcounter ST1CNT
"ST1CNT = trunc(td1 / 2)"
"MCREST = d11 - d11"
"MCWRK = 0.066667*d11"
# 86 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
1 ze
# 1 "mc_line 86 file /root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi exp. def. of mc cmd. after ze"
LBLAV, MCWRK
      "d0=d0orig + t1loop * in0 "
      "phval5 = (ph1loop % 2) * 180"
      MCWRK ip5 + phval5
      "phval3 = (t1loop * 180)%360"
      MCWRK ip3 + phval3
      "phval6 = (t1loop * 180)%360"
      MCWRK ip6 + phval6
      "phval31 = (t1loop * 180)%360"
      MCWRK ip31 + phval31
      MCWRK
      "phval0 = ph1loop * 1"
      MCWRK setgrad EA
# 87 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
  d11 pl26:f3
# 1 "mc_line 88 file /root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi exp. start label for mc cmd."
2 MCWRK  * 7 do:f3
LBLF1, MCWRK  * 7
LBLST1, MCWRK 
  MCREST
# 89 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
3 d1
  50u setrtp0|0 ctrlgrad 0
# 92 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
  (p41:sp25 ph1)
  p29:gp3
  d16
  DELTA1 pl3:f3
  (center (p42:sp26 ph1) (p22 ph6):f3 )
  DELTA1
  p29:gp3
  d16
  (p41:sp27 ph2):f1

  p16:gp4
  d16

  (p44:sp30 ph1)
# 107 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
  (p21 ph3):f3
  DELTA
  d0 


  (center (p44:sp30 ph7) (p8:sp13 ph1):f2 )
# 117 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
  d0
  p19:gp1*EA
  d16
  (p22 ph4):f3
  d16

  (p43:sp28 ph1)

  (p21 ph4):f3
  p19:gp5
  d16
  DELTA2
  (center (p42:sp26 ph1) (p22 ph1):f3 )
  DELTA2
  p19:gp5
  d16
  (p21 ph5):f3
# 135 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
  (p43:sp29 ph2)
  p29:gp6
  d16
  DELTA3
  (center (p42:sp26 ph1) (p22 ph1):f3 )
  DELTA3
  p29:gp6
  d16
  (p43:sp28 ph1)
  DELTA4
  (p42:sp26 ph1)
  4u
  p19:gp2
  d16 pl26:f3
  4u setrtp0^0 ctrlgrad 4047
  go=2 ph31 cpd3:f3
# 1 "mc_line 151 file /root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi exp. mc cmd. in line"
  MCWRK  do:f3 wr #0 if #0 zd 

  "ph1loop+=1"
      "d0=d0orig + t1loop * in0 "
      "phval5 = (ph1loop % 2) * 180"
      MCWRK ip5 + phval5
      "phval3 = (t1loop * 180)%360"
      MCWRK ip3 + phval3
      "phval6 = (t1loop * 180)%360"
      MCWRK ip6 + phval6
      "phval31 = (t1loop * 180)%360"
      MCWRK ip31 + phval31
      MCWRK
      "phval0 = ph1loop * 1"
      MCWRK setgrad EA
  lo to LBLF1 times 2
  MCWRK 

  "t1loop+=1"
      "d0=d0orig + t1loop * in0 "
      "phval5 = (ph1loop % 2) * 180"
      MCWRK ip5 + phval5
      "phval3 = (t1loop * 180)%360"
      MCWRK ip3 + phval3
      "phval6 = (t1loop * 180)%360"
      MCWRK ip6 + phval6
      "phval31 = (t1loop * 180)%360"
      MCWRK ip31 + phval31
      MCWRK
      "phval0 = ph1loop * 1"
      MCWRK setgrad EA
  lo to LBLST1 times ST1CNT
  MCWRK 
  "t1loop=0"
  "ph1loop=0"
  MCWRK rf #0
  lo to LBLAV times tdav
# 153 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
exit
   

ph1=0 
ph2=1
ph3=0 2
ph4=0 0 2 2
ph5=1 1 3 3
ph6=0
ph7=0 0 2 2
ph31=0 2 2 0
  

;pl3 : f3 channel - power level for pulse (default)
;pl26: f3 channel - power level for CPD/BB low power decoupling
;sp13: f2 channel - shaped pulse 180 degree (adiabatic)
;sp25: f1 channel - shaped pulse  90 degree (Pc9_4_90.1000)
;sp26: f1 channel - shaped pulse 180 degree (Reburp.1000)
;sp27: f1 channel - shaped pulse  90 degree (Pc9_4_90.1000)
;                   for time reversed pulse
;sp28: f1 channel - shaped pulse  90 degree (Eburp2.1000)
;sp29: f1 channel - shaped pulse  90 degree (Eburp2tr.1000)
;                   for time reversed pulse
;sp30: f1 channel - shaped pulse 180 degree (Bip720,50,20.1)
;p8 : f2 channel - 180 degree shaped pulse for inversion(adiabatic)
;p16: homospoil/gradient pulse                         [1 msec]
;p19: gradient pulse 2                                 [500 usec]
;p21: f3 channel -  90 degree high power pulse
;p22: f3 channel - 180 degree high power pulse
;p29: gradient pulse 3                                 [300 usec]
;p41: f1 channel -  90 degree shaped pulse for excitation
;                      Pc9_4_90.1000             (2.2ms at 600.13 MHz)
;p42: f1 channel - 180 degree shaped pulse for refocussing
;                      Reburp.1000               (1.4ms at 600.13 MHz)
;p43: f1 channel -  90 degree shaped pulse for excitation
;                      Eburp2.1000/Eburp2tr.1000 (1.7ms at 600.13 MHz)
;p44: f1 channel - 180 degree shaped pulse for refocussing
;                      Bip720,50,20.1            (200us at 600.13 MHz)
;d0 : incremented delay (2D)                           [3 usec]
;d1 : relaxation delay; 1-5 * T1
;d11: delay for disk I/O                               [30 msec]
;d16: delay for homospoil/gradient recovery
;d26: 1/(4J(NH))
;cnst4: = J(NH)
;cnst40: compensation of chemical shift evolution during p42
;           Reburp.1000: 0.5
;           compensate to the extend the other delays allow
;cnst41: compensation of chemical shift evolution during p41
;           Pc9_4_90.1000: 0.529
;cnst42: compensation of chemical shift evolution during p42
;           Reburp.1000: 0.5
;cnst43: compensation of chemical shift evolution during p43
;           Eburp2.1000: 0.5
;cnst51: scaling factor for p41 to compensate for transition region
;           Pc9_4_90.1000: 1.172
;cnst52: scaling factor for p42 to compensate for transition region
;           Reburp.1000: 1.426
;cnst53: scaling factor for p43 to compensate for transition region
;           Eburp2.1000: 1.000
;cnst54: H(N) chemical shift (offset, in ppm)
;cnst55: H(N) bandwidth (in ppm)
;inf1: 1/SW(N) = 2 * DW(N)
;in0: 1/(2 * SW(N)) = DW(N)
;nd0: 2
;ns: 1 * n
;ds: >= 16
;aq: <= 50 msec
;td1: number of experiments
;FnMODE: echo-antiecho
;cpd3: decoupling according to sequence defined by cpdprg3: garp4.p62
;pcpd3: f3 channel - 90 degree pulse for decoupling sequence
# 226 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
;use gradient ratio:
;  gp 1 : gp 2 : gp 3 : gp 4 : gp 5 : gp6
;    80 :  8.1 :  -13 :   50 :  -33 :  -2

;use gradient ratio

;for z-only gradients:
;gpz1: 80%
;gpz2: 8.1% for N-15
;gpz3: -13%
;gpz4: 50%
;gpz5: -33%
;gpz6: -2%
# 240 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
;use gradient files:   
;gpnam1: SMSQ10.100
;gpnam2: SMSQ10.100
;gpnam3: SMSQ10.32
;gpnam4: SMSQ10.100
;gpnam5: SMSQ10.100
;gpnam6: SMSQ10.32


                                          ;preprocessor-flags-start
;1: for calculation of all bandselective Proton pulses based on cnst54 and cnst55
;             option -DCALC_SP (eda: ZGOPTNS)
;1: for C-13 and N-15 labeled samples start experiment with 
;             option -DLABEL_CN (eda: ZGOPTNS)
                                          ;preprocessor-flags-end
# 258 "/root/.topspin-BladeEpu/local_acqu/ppDirs/b_hsqcetf3gpsi"
